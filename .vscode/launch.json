    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Current File with no args",
        "type": "debugpy",
        "request": "launch",
        "program": "${file}",
        "args": ["none"],
        "console": "integratedTerminal"
    },
    {
        "name": "Python: Current File with ARCH FILES arg",
        "type": "debugpy",
        "request": "launch",
        "program": "${file}",
        "args": ["archive_files"],
        "console": "integratedTerminal"
    },
    {
      "name": "Python: Current File with ARCH DATA MYSQL arg",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "args": ["archive_data"],
      "console": "integratedTerminal"
  }
    ],
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
          "source": "PowerShell",
          "icon": "terminal-powershell",
          "path": "D:/Program Files/PowerShell/7/pwsh.exe"
        },
    }
}